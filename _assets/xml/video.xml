<control name="frame,frameBack,md_thumbnail,md_description,lineUp,lineDown">
<storyboard event="open">
<animation property="opacity" from="0" to="0" duration="250" mode="easeIn"/>
<animation property="opacity" from="0" to="1" duration="400" mode="easeIn" begin="250"/>
<animation property="offsetX" from="0.5" to="0" duration="400" mode="easeOutCubic" begin="250"/>
</storyboard>
<storyboard event="activateNext">
<animation property="offsetY" from="1" duration="400" mode="easeOutCubic"/>
</storyboard>
<storyboard event="activatePrev">
<animation property="offsetY" from="-1" duration="400" mode="easeOutCubic"/>
</storyboard>
<storyboard event="deactivateNext">
<animation property="offsetY" to="-1" duration="400" mode="easeOutCubic"/>
</storyboard>
<storyboard event="deactivatePrev">
<animation property="offsetY" to="1" duration="400" mode="easeOutCubic"/>
</storyboard>
</control>
				
